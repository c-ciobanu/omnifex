{
  "name": "@omnifex/auth",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./env.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit",
    "generate": "pnpx @better-auth/cli generate --config script/auth-cli.ts --output ../db/src/schema/auth.prisma"
  },
  "dependencies": {
    "@omnifex/db": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "better-auth": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@better-auth/cli": "catalog:",
    "@omnifex/eslint-config": "workspace:*",
    "@omnifex/prettier-config": "workspace:*",
    "@omnifex/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@omnifex/prettier-config"
}
